{
  "name": "roundhouse",
  "description": "roundhouse front end build system",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config client/config/webpack.config.js --env.dev",
    "watch": "webpack --config client/config/webpack.config.js -d --watch --colors --env.watch",
    "build": "webpack --config client/config/webpack.config.js -p --env.production"
  },
  "engines": {
    "node": ">=7.7.0"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-preset-env": "1.2.2",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.27.3",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.7.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "node-sass": "4.5.1",
    "postcss-loader": "1.3.3",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.0",
    "stylelint-webpack-plugin": "0.7.0",
    "url-loader": "^0.5.8",
    "webpack": "2.2.0",
    "webpack-git-hash": "1.0.2",
    "webpack-livereload-plugin": "0.10.0",
    "webpack-stats-plugin": "0.1.4"
  },
  "dependencies": {
    "webfontloader": "^1.6.27"
  },
  "browserslist": [
    "last 3 versions",
    "iOS 8"
  ]
}